<div class="container">
  <div class="row">
    <div class="col m12">
      <div class="card">
        <div class="card-content">
          <div class="page-header">
            <h1>Documento</h1>
          </div>
          <%= form_for [@entry,@document] do |f| %>
            <div class="input-field">
              <%= f.label :title, "Nombre del archivo" %>
              <%= f.text_field :title, class: 'form-control', :required => true%>
            </div>

            <div class="form-group">
                <%= f.label :ec_data, "Documento" %>
                <%= f.file_field :ec_data, class: 'form-control', :required => true%>
            </div>
            <br>
            <%= link_to "Regresar", @entry , class: "btn btn-primary"%>
            <%= f.submit 'Subir', class: 'btn btn-primary' %>
          <%end%>
        </div>
      </div>
    </div>
  </div>
</div>